{
  "manifest_version": 2,
  "name": "Tabby",
  "version": "0.1",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.0.0.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "tabby.png"
  }
}

